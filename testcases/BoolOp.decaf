class Main {
    static bool f() {
        Print(1);
        return true;
    }

    static bool g() {
        Print(2);
        return false;
    }

    static void main() {
        if (f() || g()) {
            Print("hello");
        } else {
            Print("goodbye");
        }
        if (g() && f()) {
            Print("hell");
        } else {
            Print("world");
        }
    }
}